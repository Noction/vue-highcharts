{
  "name": "@noction/vue-highcharts",
  "version": "1.0.3",
  "description": "Vue wrapper for Highcharts",
  "type": "module",
  "main": "dist/main.cjs",
  "types": "dist/main.d.ts",
  "packageManager": "pnpm@8.7.1",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs",
      "default": "./dist/main.cjs"
    },
    "./*": "./*"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Noction/vue-highcharts"
  },
  "homepage": "https://github.com/Noction/vue-highcharts",
  "bugs": "https://github.com/Noction/vue-highcharts/issues",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint . --ext .ts",
    "type:check": "tsc",
    "test": "eslint tests --ext .spec.ts && tsc && vitest run --globals --environment=jsdom",
    "test:coverage": "pnpm test -- --coverage.enabled --coverage.provider=c8 --coverage.reporter=cobertura",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "vue-highcharts",
    "highcharts-vue",
    "highcharts",
    "vue-charts",
    "wrapper",
    "vue",
    "component",
    "charts",
    "vue3"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-c8": "^0.31.4",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "jsdom": "^22.1.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.31.4"
  },
  "peerDependencies": {
    "highcharts": ">=10",
    "vue": ">=3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "engines": {
    "node": "^20",
    "pnpm": "^8"
  }
}
